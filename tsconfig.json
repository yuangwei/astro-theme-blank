{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
			"@/*": ["./src/*"],
		}
  },
  "include": ["src", "*.ts", "src/content"],
  "exclude": ["node_modules", "**/node_modules/*", ".vscode", "dist"]
}